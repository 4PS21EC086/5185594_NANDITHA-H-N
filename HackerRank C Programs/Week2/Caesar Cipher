#include <stdio.h>
#include <string.h>
#include <ctype.h>

void caesarCipher(char *s, int k) {
    int n = strlen(s);
    k = k % 26;  // ensure rotation within alphabet

    for (int i = 0; i < n; i++) {
        if (isalpha(s[i])) {
            if (isupper(s[i])) {
                s[i] = ((s[i] - 'A' + k) % 26) + 'A';
            } else if (islower(s[i])) {
                s[i] = ((s[i] - 'a' + k) % 26) + 'a';
            }
        }
        // leave non-alphabetic characters unchanged
    }
}

int main() {
    int n, k;
    char s[105];  // string length up to 100

    scanf("%d", &n);
    scanf("%s", s);
    scanf("%d", &k);

    caesarCipher(s, k);
    printf("%s\n", s);

    return 0;
}
