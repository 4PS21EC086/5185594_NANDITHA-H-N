#include <stdio.h>
#include <stdlib.h>

int sockMerchant(int n, int ar_count, int* ar) {
    int pairs = 0;
    // Assuming sock colors are in a range like 1 to 100
    int frequency[101] = {0}; 
    
    // Count the frequency of each sock color
    for (int i = 0; i < n; i++) {
        frequency[ar[i]]++;
    }
    
    // Calculate the number of pairs for each color
    for (int i = 0; i < 101; i++) {
        pairs += frequency[i] / 2;
    }
    
    return pairs;
}

int main() {
    int n;
    scanf("%d", &n);
    
    int* ar = (int*)malloc(n * sizeof(int));
    for (int i = 0; i < n; i++) {
        scanf("%d", &ar[i]);
    }
    
    int result = sockMerchant(n, n, ar);
    printf("%d\n", result);
    
    free(ar);
    return 0;
}
