#include <stdio.h>
#include <stdlib.h>

int month[] = {31,28,31,30,31,30,31,31,30,31,30,31};

int isLeap(int y) {
    if (y % 400 == 0) return 1;
    if (y % 100 == 0) return 0;
    if (y % 4 == 0) return 1;
    return 0;
}

int storeDate(int d, int m, int y) {
    long long x = d;
    x = x * 100 + m;
    x = x * 10000 + y;   // concatenate dd, mm, yyyy
    if (x % 4 == 0 || x % 7 == 0) return 1;
    return 0;
}

int main() {
    int d1, m1, y1;
    int d2, m2, y2;
    scanf("%d-%d-%d", &d1, &m1, &y1);
    scanf("%d-%d-%d", &d2, &m2, &y2);

    int ans = 0;

    while (1) {
        ans += storeDate(d1, m1, y1);

        if (d1 == d2 && m1 == m2 && y1 == y2) break;

        d1++;
        if (m1 == 2 && isLeap(y1)) {
            if (d1 > 29) { d1 = 1; m1++; }
        } else {
            if (d1 > month[m1 - 1]) { d1 = 1; m1++; }
        }

        if (m1 > 12) { m1 = 1; y1++; }
    }

    printf("%d\n", ans);
    return 0;
}
