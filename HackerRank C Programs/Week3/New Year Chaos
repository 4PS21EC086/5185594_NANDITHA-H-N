#include <stdio.h>
#include <stdlib.h>

void minimumBribes(int arr_count, int* arr) {
    int bribes = 0;
    int chaotic = 0;

    for (int i = 0; i < arr_count; i++) {
        
        if (arr[i] - (i + 1) > 2) {
            chaotic = 1;
            break;
        }


        for (int j = i - 1; j >= 0; j--) {
            if (arr[j] > arr[i]) {
                bribes++;
            }
        }
    }

    if (chaotic) {
        printf("Too chaotic\n");
    } else {
        printf("%d\n", bribes);
    }
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int* q = (int*)malloc(n * sizeof(int));
        for (int i = 0; i < n; i++) {
            scanf("%d", &q[i]);
        }
        minimumBribes(n, q);
        free(q);
    }
    return 0;
}
