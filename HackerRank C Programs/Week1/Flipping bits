#include <stdio.h>
#include <stdint.h>

static inline uint32_t flippingBits(uint32_t n) {
    return ~n;                 // same as n ^ 0xFFFFFFFFu
}

int main(void) {
    int q;
    if (scanf("%d", &q) != 1) return 0;

    while (q--) {
        uint32_t n;
        scanf("%u", &n);       // read 32-bit unsigned
        uint32_t res = flippingBits(n);
        printf("%u\n", res);   // print as unsigned
    }
    return 0;
}
