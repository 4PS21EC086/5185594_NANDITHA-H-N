#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
    int n;
    scanf("%d", &n);

    char **strings = malloc(n * sizeof(char *));
    for (int i = 0; i < n; i++) {
        strings[i] = malloc(25 * sizeof(char));
        scanf("%24s", strings[i]);
    }

    int q;
    scanf("%d", &q);

    char query[25];
    for (int k = 0; k < q; k++) {
        scanf("%24s", query);
        int count = 0;
        for (int i = 0; i < n; i++) {
            if (strcmp(strings[i], query) == 0) {
                count++;
            }
        }
        printf("%d\n", count);
    }

    for (int i = 0; i < n; i++) {
        free(strings[i]);
    }
    free(strings);

    return 0;
}
