#include <stdio.h>

void countingSort(int arr[], int n) {
    int result[100] = {0};  // Step 1: Initialize frequency array with 100 zeros

    // Step 2: Count occurrences of each number
    for (int i = 0; i < n; i++) {
        result[arr[i]]++;
    }

    // Step 3: Print the frequency array
    for (int i = 0; i < 100; i++) {
        printf("%d ", result[i]);
    }
}

int main() {
    int n;
    scanf("%d", &n);   // number of items
    
    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    countingSort(arr, n);
    
    return 0;
}
